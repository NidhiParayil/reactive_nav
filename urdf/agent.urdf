<?xml version="1.0" ?>
<robot name="agent">
  <link name="world"/>
  <link name="base">
    <visual>
      <geometry>
        <mesh filename="package://home/nidhi/pybullet_ws/model/cube.obj" scale = "10 10 .01"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 1"/>
      <material name="LightGrey">
        <color rgba="0. 0.7 0.7 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://home/nidhi/pybullet_ws/model/cube.obj" scale = "10 10 .01"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 1"/>
    </collision>
  </link>
     
  <joint name="base_joint" type="fixed">
    <parent link="world"/>
    <child link="base"/>
  </joint>
    <link name="agent_dummy"/>

 <joint name="joint1" type="prismatic">
    <parent link="base"/>
    <child link="agent_dummy"/>
    <axis xyz ="1 0 0"/> 
    <limit effort="1000.0" lower="-10" upper="10" velocity=".1"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
  </joint>

  <link name="agent">
    <visual>
      <geometry>
        <mesh filename="package://home/nidhi/pybullet_ws/model/cube.obj" scale = "1. 1. 1."/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 3"/>
      <material name="green">
        <color rgba="0 1 0 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://home/nidhi/pybullet_ws/model/cube.obj" scale = "1. 1. 1."/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 3"/>
    </collision>
  </link>

 <joint name="joint2" type="prismatic">
    <parent link="agent_dummy"/>
    <child link="agent"/>
    <axis xyz ="0 1 0"/> 
    <limit effort="1000.0" lower="-15" upper="5" velocity=".1"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
  </joint>


  <link name="obstacle">
    <visual>
      <geometry>
        <mesh filename="package://home/nidhi/pybullet_ws/model/cube.obj" scale = "1 1 1.5"/>
      </geometry>
      <origin rpy="0 0 0" xyz="2 2.5 3"/>
      <material name="red">
        <color rgba="1 0 0 .9"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://home/nidhi/pybullet_ws/model/cube.obj" scale = "1 1 1.5"/>
      </geometry>
      <origin rpy="0 0 0" xyz="2 2.5 3"/>
    </collision>
  </link> 

  <joint name="world_ob" type="prismatic">
    <parent link="world"/>
    <child link="obstacle"/>
    <limit effort="1000.0" lower="-.01" upper="1" velocity=".1"/>
    <axis xyz ="0 1 0"/> 
  </joint>
